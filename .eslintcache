,\n  selectCartTotal,\n} from '../../redux/cart/cart-selectors'\nimport cartItem from '../../components/cart-item/CartItem'\n\nconst CheckoutPage = ({ cartItems, total }) => {\n  return (\n    <div className='checkout-page'>\n      <div className='checkout-header'>\n        <div className='header-block'>\n          <span>Product</span>\n        </div>\n        <div className='header-block'>\n          <span>Description</span>\n        </div>\n        <div className='header-block'>\n          <span>Quantity</span>\n        </div>\n        <div className='header-block'>\n          <span>Price</span>\n        </div>\n        <div className='header-block'>\n          <span>Remove</span>\n        </div>\n      </div>\n      {cartItems.map((cartItem) => (\n        <CheckoutItem key={cartItem.id} cartItem={cartItem} />\n      ))}\n      <div className='total'>\n        <span>TOTAL: ${total}</span>\n      </div>\n      <div className='test-warning'>\n        TEST CREDIT CARD\n        <br />\n        4242 4242 4242 4242 - expiry: 01/22 - CVV: 123\n      </div>\n      <StripeButton price={total} />\n    </div>\n  )\n}\n\nconst mapStateToProps = createStructuredSelector({\n  cartItems: selectCartItems,\n  total: selectCartTotal,\n})\n\nexport default connect(mapStateToProps)(CheckoutPage)\n",["178","179"],"/Users/radbahi/crwn-clothing/src/components/checkout-item/CheckoutItem.js",[],"/Users/radbahi/crwn-clothing/src/redux/store.js",[],"/Users/radbahi/crwn-clothing/src/redux/root-reducer.js",[],"/Users/radbahi/crwn-clothing/src/redux/directory/directory-selectors.js",[],"/Users/radbahi/crwn-clothing/src/redux/directory/directory-reducers.js",[],"/Users/radbahi/crwn-clothing/src/redux/shop/shop-selectors.js",[],"/Users/radbahi/crwn-clothing/src/components/collections-overview/CollectionsOverview.js",[],"/Users/radbahi/crwn-clothing/src/pages/collection/Collection.js",[],["180","181"],"/Users/radbahi/crwn-clothing/src/redux/user/user-actions.js",[],"/Users/radbahi/crwn-clothing/s